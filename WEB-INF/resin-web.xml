<web-app xmlns="http://java.sun.com/xml/ns/j2ee" version="2.4"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http:/java.sun.com/dtd/web-app_2_3.dtd">
  <servlet>
    <servlet-name>registrar</servlet-name>
    <servlet-class>edu.drexel.cs461.examples.RegistrarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>registrar</servlet-name>
	  <url-pattern>/registrar</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>SongListServlet</servlet-name>
    <servlet-class>playlist.SongListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>SongListServlet</servlet-name>
	  <url-pattern>/songlist</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PlaylistDisplayServlet</servlet-name>
    <servlet-class>playlist.PlaylistDisplayServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>PlaylistDisplayServlet</servlet-name>
	  <url-pattern>/playlistdisplay</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PlaylistInsertServlet</servlet-name>
    <servlet-class>playlist.PlaylistInsertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>PlaylistInsertServlet</servlet-name>
	  <url-pattern>/playlistinsert</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PlaylistRemoveServlet</servlet-name>
    <servlet-class>playlist.PlaylistRemoveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>PlaylistRemoveServlet</servlet-name>
	  <url-pattern>/playlistremove</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PlaylistCreateServlet</servlet-name>
    <servlet-class>playlist.PlaylistCreateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>PlaylistCreateServlet</servlet-name>
	  <url-pattern>/playlistcreate</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PlaylistDeleteServlet</servlet-name>
    <servlet-class>playlist.PlaylistDeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>PlaylistDeleteServlet</servlet-name>
	  <url-pattern>/playlistdelete</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>test.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>hello</servlet-name>
	  <url-pattern>/hello</url-pattern>
  </servlet-mapping>
</web-app>
